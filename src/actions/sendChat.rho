match ["$channel", "$message"] {
[channel, message] => {
new
   return(`rho:rchain:deployId`),
   deployerId(`rho:rchain:deployerId`),
   stdout(`rho:io:stdout`),
   ch
in {
   for(@{"peek": *peek, ..._} <<- @[*deployerId, "inbox"]) {
      return!("return one") |
      peek!("chat", channel, *ch) |
      for (@[{"pub": pub, ..._}] <- ch) {
       return!("return two") |
          @pub!(message)
      }
   }
}
}} // end of match
